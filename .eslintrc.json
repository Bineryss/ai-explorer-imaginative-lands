{
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/eslint-config-typescript",
    "@vue/eslint-config-prettier",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "vue/no-unused-vars": "error",
    "semi": [
      "error",
      "single"
    ],
    "quotes": [
      "error",
      "double"
    ]
  }
}
